@model NRDCL.Models.Customer

@{
    ViewData["Title"] = "Customer";
}

<div>
    <h4>Customer Detail</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Citizenship ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CitizenshipID)
        </dd>
        <dt class="col-sm-2">
           Customer Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CustomerName)
        </dd>
        <dt class="col-sm-2">
            Telephone Number
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TelephoneNumber)
        </dd>
        <dt class="col-sm-2">
           Email ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EmailId)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { CitizenshipID = Model.CitizenshipID }) |
    <a asp-action="Index">Back to List</a>
</div>
